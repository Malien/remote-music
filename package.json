    "url": "https://github.com/Malien/remote-music/issues"
  },
  "homepage": "https://github.com/Malien/remote-music#readme",
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/ws": "^6.0.1",
    "electron": "^4.2.6",
    "electron-packager": "^13.1.1",
    "source-map-loader": "^0.2.4",
    "terser": "^4.1.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
