  "description": "Music client with remote commands support",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "tsc; echo \"Typescript compilation finished!\n\"; webpack;mkdir -p dist/app/views; find src/app/views -not -name \"*.ts\" -type f -exec cp -fR {} dist/app/views/ \\;",
